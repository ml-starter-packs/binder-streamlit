# get app and requirements any way you want here.
curl https://raw.githubusercontent.com/streamlit/example-app-time-series-annotation/main/requirements.txt > requirements.txt
pip install -r requirements.txt
curl https://raw.githubusercontent.com/streamlit/example-app-time-series-annotation/main/streamlit_app.py > app.py
# upgrade click
pip install click==8.1.0
